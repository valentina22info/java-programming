import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner ph =  new Scanner(System.in);
        int a = ph.nextInt();
        int c = 0;
        System.out.println("The Count the digits: " + c(Math.abs(a)));
    }
    static int c(int a){
        if(a>1){
            return 1+ c(a/10);
        }
        else{
            return 1;
        }
        }
        }
    
