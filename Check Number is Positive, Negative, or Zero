import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ph = new Scanner(System.in);
        int n = ph.nextInt();
        if(n == 0){
            System.out.println("zero");
        }
        else if(n>0){
            System.out.println("positive");

        }
        else{
            System.out.println("negative");

        }
    }
}
     
