import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner pp = new Scanner(System.in);
        int hours = pp.nextInt();
        int minutes = pp.nextInt();
        int seconds = pp.nextInt();
        minutes += seconds / 60;
        seconds = seconds % 60;
        hours += minutes / 60;
        minutes = minutes % 60;
        System.out.println("Total Number of hours is " + hours);
        System.out.println("Total Number of minutes is " + minutes);
        System.out.println("Total Number of seconds is " + seconds);
    }
}
