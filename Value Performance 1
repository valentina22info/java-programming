import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ks = new Scanner (System.in);
        int b = ks.nextInt();
        float c = ks.nextFloat();
        System.out.println(b);
        System.out.printf("%.2f",c);
    }
}
